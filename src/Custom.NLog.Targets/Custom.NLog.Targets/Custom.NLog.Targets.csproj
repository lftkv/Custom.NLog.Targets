<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp1.1</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance>
    <Version>1.0.0-alpha</Version>
    <Authors>lftkv</Authors>
    <Company>lukascoding</Company>
    <Description>custom nlog target for telegram</Description>
    <PackageProjectUrl>https://github.com/lftkv/Custom.NLog.Targets</PackageProjectUrl>
    <Copyright>lukascoding</Copyright>
    <PackageTags>nlog telegram channel chat group logging bot</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NLog.Web.AspNetCore" Version="4.3.1" />
    <PackageReference Include="Telegram.Bot" Version="10.4.0" />
  </ItemGroup>

</Project>